<%= render_erb( Dir.pwd + '/views/appledoc_template/_head.html', "heirarchy") %>

	<body>

    <%= render_erb( Dir.pwd + '/views/appledoc_template/_header.html').gsub('class="hideInXcode"', 'class="hideInXcode minimised"') %>


		<article>
			<div id="overview_contents" role="main">

				<div id="header">
					<div class="section-header">
						<h1 class="title title-header">{{page.title}}</h1>
					</div>
				</div>

				<div id="container">
					{{#hasClasses}}
					<div class="hierarchy-column">
						<h2 class="index-title">{{strings.hierarchyPage.classesTitle}}</h2>
						{{>Classes}}
					</div>
					{{/hasClasses}}

					{{#hasProtocolsOrCategories}}
					<div class="index-column">
						{{#hasProtocols}}
						<h2 class="index-title">{{strings.hierarchyPage.protocolsTitle}}</h2>
						<ul>
							{{#protocols}}
							<li><a href="{{href}}">{{title}}</a></li>
							{{/protocols}}
						</ul>
						{{/hasProtocols}}

						{{#hasCategories}}
            </div>
  					<div class="index-column">

						<h2 class="index-title">{{strings.hierarchyPage.categoriesTitle}}</h2>
						<ul>
							{{#categories}}
							<li><a href="{{href}}">{{title}}</a></li>
							{{/categories}}
						</ul>
						{{/hasCategories}}

					</div>
					{{/hasProtocolsOrCategories}}

					{{#hasConstants}}
  					<div class="index-column">
						<h2 class="index-title">{{strings.indexPage.constantsTitle}}</h2>
						<ul>
							{{#constants}}
							<li><a href="{{href}}">{{title}}</a></li>
							{{/constants}}
						</ul>
					</div>
					{{/hasConstants}}

				</div>

				<%= File.read( Dir.pwd + '/views/appledoc_template/_footer.html') %>

			</div>
		</article>

    <%= render_erb( Dir.pwd + '/views/appledoc_template/_final-scripts.html') %>

	</body>
</html>

Section Classes
{{#hasClasses}}
	<ul>
	{{#classes}}
		<li>{{#href}}<a href="{{href}}">{{/href}}{{name}}{{#href}}</a>{{/href}}{{>Classes}}</li>
	{{/classes}}
	</ul>
{{/hasClasses}}
EndSection

Section Navigation

EndSection
