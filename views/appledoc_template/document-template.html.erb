<%= render_erb( Dir.pwd + '/views/appledoc_template/_head.html').gsub('href="appledoc_stylesheet', 'href="../../appledoc_stylesheet' ) %>

    <body>

    <%= render_erb( Dir.pwd + '/views/appledoc_template/_header.html', "document").gsub('class="hideInXcode"', 'class="hideInXcode minimised"') %>

		<article>
		{{#page}}<h1><a id="libraryTitle" href="{{documentationIndexPath}}">{{projectName}} {{strings/objectPage/libraryTitlePostfix}}</a></h1>
		<a id="developerHome" href="{{documentationIndexPath}}">{{projectCompany}}</a>{{/page}}

			<a title="{{page/title}}" name="top"></a>
			<div id="overview_contents" role="main">
				<div id="container">
					{{#object/comment}}{{#longDescription}}{{>GBCommentComponentsList}}{{/longDescription}}{{/object/comment}}
				</div>

                <%= render_erb( Dir.pwd + '/views/appledoc_template/_footer.html') %>

			</div>
		</article>

    <%= render_erb( Dir.pwd + '/views/appledoc_template/_final-scripts.html') %>

	</body>
</html>


Section GBCommentComponentsList
{{#components}}{{>GBCommentComponent}}{{/components}}
EndSection

Section GBCommentComponent
{{&htmlValue}}
EndSection
